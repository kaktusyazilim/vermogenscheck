<main>
    <section class="landing-sec">
        <div class="inner">
            <div class="bg">
                <div class="image">
                    <img src="/themes/uploads/landing2.webp" alt="">
                </div>
            </div>
            <div class="head">
                <div class="left">
                    <h1><span>Für Ihre</span> Investitionen<br>Die richtige Adresse</h1>
                    <p>Unser tiefgründiges und unabhängiges Team von Finanzexperten werden euch Weiterhelfen.</p>
                    <a href="/contact" class="btn">Jetzt Loslegen</a>
                </div>
            </div>

            <div class="body">
                <div class="exchanges" data-flickity='{"pageDots": false, "autoPlay": 2000, "pauseAutoPlayOnHover": false, "prevNextButtons": false ,"cellAlign": "left","contain": true, "wrapAround": true,"initialIndex": 0}'>
                    @foreach (var item in Model.ExchangesList)
                    {
                        <a class="exchange" href="/form?Exchange=@item.ItemGuid">
                            <div class="icon">
                                <img src="@item.Icon" alt="">
                            </div>
                            <div class="content">
                                <h3>Festverzinsliche Einlagen in Deutschland jährlich <span>@item.Count%</span></h3>
                            </div>
                        </a>
                    }
                </div>
            </div>

        </div>
    </section>




    <section class="whywe-sec">
        <div class="left">
            <div class="image">
                <img src="@Model.AboutUs.Image" alt="">
            </div>
        </div>
        <div class="right">
            <h2>
                @Model.AboutUs.Title
            </h2>
            <p>
                @Model.AboutUs.Description
            </p>
            <a href="/contact" class="btn">Erfahren Sie mehr</a>
        </div>
    </section>


    <section class="exchanges-sec">


        <div class="cards">

            @foreach (var item in Model.ExchangesList)
            {
                <div class="card">
                    <div class="icon">
                        <img src="@item.Icon" alt="">
                    </div>
                    <div class="count">
                        @item.Name <span>@item.Count%</span>
                    </div>
                    <div class="desc">
                        EU-Einlagensicherung bis <span>100.000 €</span> Zinsertrag <span>@(100000 * item.Count)€</span>
                    </div>
                    <div class="link">
                        <a href="/form?Exchange=@item.ItemGuid" class="btn">Mehr erfahren</a>
                    </div>
                </div>
            }
        </div>

    </section>


